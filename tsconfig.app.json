{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.ts", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*", "src/services/dtos/responseDtos/*"],
  "compilerOptions": {
    "skipLibCheck": false,
    "strictNullChecks": true,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "es2021",
    "lib": ["es2021", "dom", "ES2021.String"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*", "./src/*.vue"],
      "@models/*": ["./src/models/*"],
      "@requestDtos/*": ["./src/services/dtos/requestDtos/*"],
      "@responseDtos/*": ["./src/services/dtos/responseDtos/*"],
      "@enums": ["./src/services/dtos/enums"],
      "@forms/*": ["./src/components/formInputs/*"],
      "@layouts/*": ["./src/views/layouts/*"],
    },
  },
}
